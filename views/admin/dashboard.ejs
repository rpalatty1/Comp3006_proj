<div class="admin-title">
    <h2> Bookings </h2>
    <a href="/add-post" class="button">+ Add booking</a>
</div>
<ul class="admin-posts">

    <% data.forEach(post => { %>

        <li>

            <a href="/post/<%= post._id %>">
                <%= post.startLocation %> &nearr; 
            
            </a>

            <div class="admin-post-controls">
                <a href="/edit-post/<%= post.id %>" class="btn"> Edit</a>

                <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="Delete" class="btn">
                </form>

            </div>
        </li>
     
    <% }) %>

</ul>